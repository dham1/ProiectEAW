{% extends 'base.html.twig' %}

{% block title %}Examination{% endblock %}

{% block body %}
    {% include('base.html.twig') %}

    <h1>Examination</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ examination.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ examination.Date ? examination.Date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ examination.Description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('examination_index') }}">back to list</a>

    <a href="{{ path('examination_edit', {'id': examination.id}) }}">edit</a>

    {{ include('examination/_delete_form.html.twig') }}
{% endblock %}
